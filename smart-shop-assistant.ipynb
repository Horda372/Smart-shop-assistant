def scroll_window():
    """Przewija ca stron przegldarki do dou."""
    display(Javascript("window.scrollTo(0, document.body.scrollHeight);"))

def handle_ai_response():
    global chat_history
    try:
        response = client.chat.completions.create(
            model="gpt-4o-mini",
            messages=chat_history,
            tools=tools
        )
        assistant_msg = response.choices[0].message

        if assistant_msg.content:
            content = assistant_msg.content
            chat_history.append({"role": "assistant", "content": content})
            with output_log:
                display(HTML(f"""
                <div style='text-align: left; margin: 15px 5px;'>
                    <div style='background-color: #f1f2f6; color: #2f3542; padding: 12px 18px; border-radius: 15px; border-left: 5px solid #2ecc71; display: inline-block; max-width: 85%; font-family: sans-serif; box-shadow: 2px 2px 8px rgba(0,0,0,0.05);'>
                        <b style='color: #27ae60;'> Doradca Terra-AI:</b><br>{content}
                    </div>
                </div>
                """))
            scroll_window()

        if assistant_msg.tool_calls:
            chat_history.append(assistant_msg)
            for tool_call in assistant_msg.tool_calls:
                if tool_call.function.name == "generuj_koszyk_json":
                    args = json.loads(tool_call.function.arguments)
                    with output_log:
                        display(HTML(f"""
                        <div style='background-color: #2d3436; padding: 15px; border-radius: 10px; margin: 10px 0; border: 1px solid #55efc4; font-family: monospace;'>
                            <b style='color: #55efc4;'> SYSTEM: Wygenerowano koszyk</b>
                            <pre style='color: #ffffff; white-space: pre-wrap; margin-top: 10px; font-size: 0.9em;'>{json.dumps(args, indent=2, ensure_ascii=False)}</pre>
                        </div>
                        """))
                    scroll_window()
                    chat_history.append({"role": "tool", "tool_call_id": tool_call.id, "content": "Zrobione."})
            return handle_ai_response()

    except Exception as e:
        with output_log:
            print(f"Bd API: {str(e)}")

def on_send(b=None):
    query = user_input.value.strip()
    if not query: return

    with output_log:
        display(HTML(f"""
        <div style='text-align: right; margin: 15px 5px;'>
            <div style='background-color: #0984e3; color: white; padding: 12px 18px; border-radius: 15px; display: inline-block; max-width: 85%; font-family: sans-serif; box-shadow: 2px 2px 8px rgba(0,0,0,0.1);'>
                <b>Ty:</b><br>{query}
            </div>
        </div>
        """))

    chat_history.append({"role": "user", "content": query})
    user_input.value = ""
    scroll_window()

    thinking_loader.value = "<div style='color: #7f8c8d; font-style: italic; margin: 5px 10px;'> Doradca pisze...</div>"
    handle_ai_response()
    thinking_loader.value = ""

